 @inproceedings{Pradhan_Bhansali_Chandnani_Pangaonkar_2020, title={Analysis of Personality Traits using Natural Language Processing and Deep Learning}, url={https://ieeexplore.ieee.org/abstract/document/9183090}, DOI={10.1109/ICIRCA48905.2020.9183090}, abstractNote={Personality test and analysis is an important factor in an individual’s overall development. Most known personality test followed by people is Myer Briggs Test Indicator. While these tests are conducted by psychologists, it is very easy to deceive them in order to get the personality type of our choice, as these questions are very straight forward. This paper focuses on automating this task with the help of Neural Networks by using images instead of questions. A labelled dataset with user responses on social media along with their personality type is used for analysis. After cleaning, relevant response features are extracted using NLP followed by applying suitable classification algorithms. A model is trained to accurately predict the personality type of users based on their responses, which is then deployed on an interactive website in the form of a personality test.}, booktitle={2020 Second International Conference on Inventive Research in Computing Applications (ICIRCA)}, author={Pradhan, Tejas and Bhansali, Rashi and Chandnani, Dimple and Pangaonkar, Aditya}, year={2020}, month=july, pages={457–461} }



